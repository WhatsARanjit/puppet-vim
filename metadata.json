{
  "name": "WhatsARanjit/vim",
  "version": "0.2.3",
  "author": "WhatsARanjit",
  "summary": "Managed vim with Puppet",
  "license": "Apache 2.0",
  "source": "https://github.com/WhatsARanjit/puppet-vim",
  "project_page": "https://github.com/WhatsARanjit/puppet-vim",
